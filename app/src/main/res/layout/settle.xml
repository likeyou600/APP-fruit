<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/cleargameback">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="309dp"
        android:layout_height="73dp"
        android:background="@drawable/bg_round_button"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.005"
        app:srcCompat="@drawable/fin" />
    <TextView
        android:id="@+id/fin"
        android:layout_width="322dp"
        android:layout_height="96dp"
        android:layout_marginTop="76dp"
        android:background="@drawable/bg_round_button"
        android:textAlignment="center"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nowget" />

    <ImageButton
        android:id="@+id/back"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginStart="48dp"
        android:layout_marginBottom="28dp"
        android:background="#00FFFFFF"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/dh" />

    <ImageButton
        android:id="@+id/again"
        android:layout_width="157dp"
        android:layout_height="155dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="16dp"
        android:background="#00FFFFFF"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/ag" />

    <ImageView
        android:id="@+id/nowget"
        android:layout_width="256dp"
        android:layout_height="251dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.322"
        app:srcCompat="@drawable/f" />

    <View
        android:layout_width="268dp"
        android:layout_height="265dp"
        android:layout_centerInParent="true"
        android:background="@drawable/nofill_round"
        app:layout_constraintBottom_toBottomOf="@+id/nowget"
        app:layout_constraintEnd_toEndOf="@+id/nowget"
        app:layout_constraintHorizontal_bias="0.615"
        app:layout_constraintStart_toStartOf="@+id/nowget"
        app:layout_constraintTop_toTopOf="@+id/nowget"
        app:layout_constraintVertical_bias="0.56" />

    <ImageView
        android:id="@+id/newlogo"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginTop="110dp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.896"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/resource_new" />

</androidx.constraintlayout.widget.ConstraintLayout>